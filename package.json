{
  "name": "goodtimes",
  "version": "0.1.0",
  "description": "A DataSupplier plugin",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "GoodTimes",
    "manifest": "src/manifest.json",
    "main": "goodtimes.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.5"
  },
  "author": "chewbecca <beckybettinger@yahoo.de>",
  "dependencies": {
    "prettier": "^1.19.1"
  }
}
